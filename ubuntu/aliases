
unalias -a

alias ..='cd ..'
alias ...='cd ../..'
alias cp='cp -i'
alias df='df -h'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias grep='grep --color=auto'
alias l='ls -CF'
alias la='ls -A'
alias ll='ls -alF'
alias ls='ls --color=auto'
alias md='mkdir -p'
alias mv='mv -i'
alias rsync='rsync -avz -u -e ssh'
alias tail='tail -fn1000'
